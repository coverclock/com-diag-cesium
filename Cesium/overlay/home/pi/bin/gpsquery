/usr/local/bin/gpstool -D /dev/ttyACM0 -b 9600 -8 -n -1 -t 10 -M 4 -E \
	-W '\$PUBX,00' \
	-W '\$PUBX,03' \
	-W '\$PUBX,04' \
	-W '\$PUBX,40,GSV,0,0,0,1,0,0' \
	-W '\$PUBX,40,VTG,0,0,0,1,0,0' \
	-W '\xb5\x62\x06\0\0\0' \
	-W '\xb5\x62\x06\x01\x03\0\x01\x20\x01' \
	-W '\xb5\x62\x06\x01\x03\0\x01\x21\x01' \
	-W '\xb5\x62\x06\x01\x03\0\x01\x22\x01' \
	-W '\xb5\x62\x06\x01\x03\0\x0d\x01\x01' \
	-W '\xb5\x62\x06\x01\x03\0\x0d\x03\x01' \
	-W '\xb5\x62\x06\x01\x03\0\x0a\x09\x01' \
	-W '\xb5\x62\x06\x01\x03\0\x01\x03\x01' \
	-W '\xb5\x62\x06\x39\x08\0\xf7\x56\xb1\x96\x1e\x63\0\0' \
	-W '\xb5\x62\x0a\x04\0\0' \
	-W '\xb5\x62\x06\x06\0\0' \
	-W '\xb5\x62\x06\x31\0\0' \
	-W '\xb5\x62\x06\x3e\0\0' \
2> >(/usr/local/bin/log -S -N gpstool)
